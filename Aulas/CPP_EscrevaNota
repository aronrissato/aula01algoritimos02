// TestandoProjeto.cpp : Define o ponto de entrada para a aplicação de console.
//

#include "stdafx.h"
#include <stdio.h>
#include <stdlib.h>




float ConsultaNota()
{
	printf("Informe sua nota: \n");
	float nota;
	scanf_s("%f", &nota);
	return nota;
}




float main() {

	printf("Digite zero para sair do programa\n");

	while (ConsultaNota() != 0) {
		do
		{
			void Consulta(); {
				if (ConsultaNota() >= 6)
				{
					printf("Aprovado.\n");
				}
				else
				{
					printf("Reprovado.\n");
				}
			}
		}

		return 0;
		system("pause");
